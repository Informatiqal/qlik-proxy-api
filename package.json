{
  "name": "qlik-proxy-api",
  "version": "1.1.2",
  "description": "Interact with Qlik Sense Proxy API (QSEoW)",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/app.js",
    "prestart": "npm run build-tsc",
    "build-tsc": "tsc",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "docs": "npx typedoc src/index.doc.ts --name \"Qlik Proxy API\" --excludePrivate --hideGenerator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informatiqal/qlik-proxy-api.git"
  },
  "keywords": [
    "proxy",
    "qlik",
    "qlik-sense",
    "rest",
    "api"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "*.json",
    "*.md",
    "LICENSE",
    "!CHANGELOG.md",
    "!tsconfig.json",
    "!tsconfig copy.json",
    "!tsconfig.test.json",
    "!index.doc.ts"
  ],
  "bugs": {
    "url": "https://github.com/Informatiqal/qlik-proxy-api/issues"
  },
  "homepage": "https://informatiqal.com/qlik-proxy-api/",
  "dependencies": {
    "qlik-rest-api": "^1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.3.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "rollup": "^2.56.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^10.1.0",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.5"
  }
}